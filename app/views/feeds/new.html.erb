<%= form_with(model: @feed, local: true) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <%= f.label :image %>
  <%= image_tag(@feed.image.url) if @feed.image.url.present?  %>
  <%= f.file_field :image, id: :feed_image %>
  <%= f.hidden_field :image_cache %>
  <%= f.submit  %>
<% end %>
